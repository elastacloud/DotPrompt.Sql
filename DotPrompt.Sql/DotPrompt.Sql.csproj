<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <PropertyGroup>
        <PackageId>DotPrompt.Sql</PackageId>
        <Version>0.1.1</Version>
        <Authors>Elastacloud</Authors>
        <Description>A SQL Store library for DotPrompt</Description>
        <PackageReadmeFile>readme.md</PackageReadmeFile>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/elastacloud/DotPrompt.Sql</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
        <None Include="../readme.md" Pack="true" PackagePath="readme.md" />
    </ItemGroup>
    
    <ItemGroup>
      <PackageReference Include="DotPrompt" Version="0.0.4.1" />
      <PackageReference Include="System.Data.SqlClient" Version="4.9.0" />
      <PackageReference Include="YamlDotNet" Version="16.3.0" />
    </ItemGroup>

</Project>
